AJA;AAKA;AAGA,IAAA,KARA,mBAQA;AAAA;;AACA,UAAA,QAAA,GAAA;AACA,QAAA,QAAA,EAAA,KAAA,SAAA,CAAA,QADA;AAEA,QAAA,QAAA,EAAA,KAAA,SAAA,CAAA;AAFA,OAAA,CADA,CAKA;;AACA,WAAA,OAAA,CAAA;AACA,QAAA,MAAA,EAAA,MADA;AAEA,QAAA,GAAA,EAAA,QAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAIA,IAJA,CAIA,UAAA,QAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,CAAA,CAAA;;AACA,YAAA,QAAA,KAAA,IAAA,EAAA;AACA,UAAA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,OAAA;AACA,SAFA,MAEA;AACA,UAAA,YAAA,CAAA,OAAA,CAAA,MAAA,EAAA,IAAA,CAAA,SAAA,CAAA,QAAA,CAAA,CAAA,CAAA,CAAA,EADA,CACA;;AACA,UAAA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,OAAA;;AACA,UAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,YAAA,IAAA,EAAA;AAAA,WAAA,EAHA,CAGA;;AACA;AACA,OAbA,EAaA,KAbA,CAaA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAfA;AAgBA;AA9BA;AAxBA,CAAA","sourcesContent":["<template>\n  <div class=\"login_container\">\n    <!-- 登录块 -->\n    <div class=\"login_box\">\n      <!-- 头像 -->\n      <div class=\"avatar_box\">\n        <img src=\"../assets/pickle.jpg\" alt/>\n      </div>\n      <!-- 表单区域 -->\n      <el-form ref=\"loginFormRef\" :model=\"loginForm\" :rules=\"loginRules\" class=\"login_form\" label-width=\"0\">\n        <!-- 用户名 -->\n        <el-form-item prop=\"username\">\n          <el-input placeholder=\"请输入用户名\" v-model=\"loginForm.username\" prefix-icon=\"iconfont icon-yonghuming\"></el-input>\n        </el-form-item>\n        <!-- 密码 -->\n        <el-form-item prop=\"password\">\n          <el-input placeholder=\"请输入密码\" v-model=\"loginForm.password\" prefix-icon=\"iconfont icon-mima\"\n                    type=\"password\"></el-input>\n        </el-form-item>\n        <!-- 按钮 -->\n        <el-form-item class=\"btns\">\n          <el-button type=\"primary\" class=\"denglu\" @click=\"login()\">登录</el-button>\n          <el-button type=\"info\" @click=\"LoginForm()\">注册</el-button>\n        </el-form-item>\n      </el-form>\n    </div>\n  </div>\n</template>\n<script>\n\nexport default {\n  // 绑定数据\n  data() {\n    return {\n      // 表单数据对象\n      loginForm: {\n        username: '',\n        password: ''\n      },\n      // 校验规则\n      loginRules: {\n        // 校验用户名\n        username: [\n          {required: true, message: \"请输入用户名\", trigger: \"blur\"}, // 必填项验证\n          {min: 4, max: 12, message: \"长度在 5 到 12 个字符\", trigger: \"blur\"} // 长度验证\n        ],\n        // 校验密码\n        password: [\n          {required: true, message: \"请输入密码\", trigger: \"blur\"}, // 必填项验证\n          {min: 4, max: 8, message: \"密码为 6~8 位\", trigger: \"blur\"} // 长度验证\n        ]\n      }\n    };\n  },\n  methods: {\n    // 注册功能页面跳转\n    LoginForm() {\n      this.$router.push({path: \"/LoginForm\"});\n    },\n    // 登录方法\n\n\n    login() {\n      let postData = {\n        username: this.loginForm.username,\n        password: this.loginForm.password\n      };\n      //console.log(postData)\n      this.request({\n        method: 'post',\n        url: '/login',\n        data: postData\n      }).then(response => {\n        console.log(response[0])\n        if (response === null) {\n          this.$message.error(\"登录失败！\");\n        } else {\n          localStorage.setItem(\"user\", JSON.stringify(response[0])) //本地存储空间存入用户信息\n          this.$message.success(\"登录成功！\");\n          this.$router.push({path: \"/Home\"}); // 页面路由跳转\n        }\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n  },\n}\n</script>\n<style lang=\"less\" scoped>\n.login_container {\n  background-color: #2b4b6b;\n  height: 100%;\n}\n\n.login_box {\n  width: 450px;\n  height: 300px;\n  background-color: #fff;\n  border-radius: 3px;\n  border-radius: 50px;\n  position: absolute; // 绝对定位\n  left: 50%;\n  top: 50%;\n  transform: translate(-50%, -50%);\n\n  .avatar_box {\n    width: 130px;\n    height: 130px;\n    border: 1px solid #eee;\n    border-radius: 50%;\n    padding: 5px;\n    box-shadow: 0 0 10px #ddd;\n    position: absolute;\n    left: 50%;\n    transform: translate(-50%, -50%);\n    background-color: rgb(238, 107, 0);\n\n    img {\n      width: 100%;\n      height: 100%;\n      border-radius: 50%;\n      background-color: #fff;\n    }\n  }\n}\n\n.btns {\n  display: flex; // 流式布局\n  justify-content: flex-end;\n}\n\n.login_form {\n  position: absolute;\n  bottom: 0%;\n  width: 100%;\n  padding: 10px;\n  box-sizing: border-box;\n}\n</style>\n"],"sourceRoot":"src/views"}]}