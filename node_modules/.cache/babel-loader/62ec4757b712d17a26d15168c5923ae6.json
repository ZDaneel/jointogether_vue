,aAAA,EAAA,KAAA,CAAA,IAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,gBAAA,GAAA,CAAA,IAAA,KAAA,KAAA,EAAA;AACA,cAAA,YAAA,CAAA,OAAA,CAAA,MAAA,EAAA,IAAA,CAAA,SAAA,CAAA,GAAA,CAAA,IAAA,CAAA,EADA,CACA;;AACA,cAAA,YAAA,CAAA,OAAA,CAAA,OAAA,EAAA,IAAA,CAAA,SAAA,CAAA,GAAA,CAAA,IAAA,CAAA,KAAA,CAAA,EAFA,CAEA;AACA;;AACA,cAAA,SAAA;;AACA,cAAA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA,EALA,CAMA;;;AACA,kBAAA,GAAA,CAAA,IAAA,CAAA,IAAA,KAAA,cAAA,EAAA;AACA,gBAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,aAAA;AACA,eAFA,MAEA;AACA,gBAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA;AACA,aAZA,MAYA;AACA,cAAA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,GAAA;AACA;AACA,WAhBA;AAiBA;AACA,OApBA;AAqBA;AAvBA;AAjBA,CAAA","sourcesContent":["<template>\n  <div class=\"wrapper\">\n    <div\n        style=\"margin: 200px auto; background-color: #fff; width: 350px; height: 350px; padding: 20px; border-radius: 10px\">\n      <div style=\"margin: 20px 0; text-align: center; font-size: 24px\"><b>登 录</b></div>\n      <el-form :model=\"user\" :rules=\"rules\" ref=\"userForm\">\n        <el-form-item prop=\"username\">\n          <el-input size=\"medium\" style=\"margin: 10px 0\" prefix-icon=\"el-icon-user\" v-model=\"user.username\"></el-input>\n        </el-form-item>\n        <el-form-item prop=\"password\">\n          <el-input size=\"medium\" style=\"margin: 10px 0\" prefix-icon=\"el-icon-lock\" show-password\n                    v-model=\"user.password\"></el-input>\n        </el-form-item>\n\n        <el-form-item style=\"margin: 10px 0;\">\n          <el-button type=\"primary\" size=\"medium\" style=\"width:100%\" autocomplete=\"off\" @click=\"login\">登录</el-button>\n        </el-form-item>\n        <el-form-item style=\"padding-top: 10px\">\n          <div style=\"float: left\">\n<!--            <el-button type=\"info\" size=\"mini\" autocomplete=\"off\" @click=\"$router.push('/retrievePassword')\">忘记密码-->\n<!--            </el-button>-->\n            <el-link type=\"white\" @click=\"$router.push('/retrievePassword')\">忘记密码</el-link>\n          </div>\n          <div style=\"float:right;\">\n<!--            <el-button type=\"warning\" size=\"mini\" autocomplete=\"off\" @click=\"$router.push('/register')\">注册</el-button>-->\n            <el-link type=\"white\" @click=\"$router.push('/register')\">还没有账号？点击注册</el-link>\n          </div>\n        </el-form-item>\n      </el-form>\n    </div>\n  </div>\n</template>\n\n<script>\nimport {setRoutes} from \"@/router\";\n\nexport default {\n  name: \"Login\",\n  data() {\n    return {\n      user: {},\n      rules: {\n        username: [\n          {required: true, message: '请输入用户名', trigger: 'blur'},\n          {min: 3, max: 10, message: '长度在 3 到 5 个字符', trigger: 'blur'}\n        ],\n        password: [\n          {required: true, message: '请输入密码', trigger: 'blur'},\n          {min: 1, max: 20, message: '长度在 1 到 20 个字符', trigger: 'blur'}\n        ],\n      }\n    }\n  },\n  methods: {\n    login() {\n      this.$refs['userForm'].validate((valid) => {\n        if (valid) {  // 表单校验合法\n          this.request.post(\"/user/login\", this.user).then(res => {\n            if (res.code === '200') {\n              localStorage.setItem(\"user\", JSON.stringify(res.data))  // 存储用户信息到浏览器\n              localStorage.setItem(\"menus\", JSON.stringify(res.data.menus))  // 存储用户信息到浏览器\n              // 动态设置当前用户的路由\n              setRoutes()\n              this.$message.success(\"登录成功\")\n              // 权限控制跳转\n              if (res.data.role === 'ROLE_STUDENT') {\n                this.$router.push(\"/front/home\")\n              } else {\n                this.$router.push(\"/\")\n              }\n            } else {\n              this.$message.error(res.msg)\n            }\n          })\n        }\n      });\n    }\n  }\n}\n</script>\n\n<style>\n.wrapper {\n  height: 100vh;\n  background: -webkit-gradient(linear, 0 0, 0 bottom, from(#ff0000), to(rgba(0, 0, 255, 0.5)));\n  overflow: hidden;\n}\n</style>\n<!--\n//background-image: linear-gradient(to bottom right, #FC466B, #3F5EFB);-->\n"],"sourceRoot":"src/views"}]}