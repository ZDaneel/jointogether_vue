   type=\"text\"\n              @click=\"dialogAdd = true\">添加\n          </el-button>\n        </el-form-item>\n\n        <el-form-item>\n          <el-button\n              class=\"el-icon-delete\"\n              type=\"text\"\n              @click=\"handleDeleteList()\">删除\n          </el-button>\n        </el-form-item>\n        <el-form-item>\n          <el-button type=\"info\" @click=\"loginout\">退出</el-button>\n        </el-form-item>\n      </el-form>\n      <el-button @click=\"exportData\" type=\"success\" icon=\"el-icon-download\">导出数据</el-button>\n      <el-table\n          id=\"rh\"\n          ref=\"multipleTable\"\n          :data=\"tableData\"\n          border\n          highlight-current-row\n          style=\"width: 100%\"\n          @selection-change=\"handleSelectionDelete\">\n        <el-table-column\n            type=\"selection\"\n            width=\"55\">\n        </el-table-column>\n        <el-table-column\n            label=\"商品编号\">\n          <template slot-scope=\"scope\">\n            <span>{{ scope.row.id }}</span>\n          </template>\n        </el-table-column>\n        <el-table-column\n            label=\"商品名称\">\n          <template slot-scope=\"scope\">\n            <span>{{ scope.row.name }}</span>\n          </template>\n        </el-table-column>\n        <el-table-column\n            label=\"商品类别\">\n          <template slot-scope=\"scope\">\n            <span>{{ scope.row.category }}</span>\n          </template>\n        </el-table-column>\n        <el-table-column\n            label=\"商品产地\">\n          <template slot-scope=\"scope\">\n            <span>{{ scope.row.area }}</span>\n          </template>\n        </el-table-column>\n        <el-table-column\n            label=\"商品价格\">\n          <template slot-scope=\"scope\">\n            <span>{{ scope.row.price }}</span>\n          </template>\n        </el-table-column>\n        <el-table-column\n            label=\"生产时间\">\n          <template slot-scope=\"scope\">\n            <span>{{ scope.row.dates }}</span>\n          </template>\n        </el-table-column>\n        <el-table-column\n            label=\"生产商\">\n          <template slot-scope=\"scope\">\n            <span>{{ scope.row.producer }}</span>\n          </template>\n        </el-table-column>\n        <el-table-column\n            label=\"操作\">\n          <template slot-scope=\"scope\">\n            <el-button\n                type=\"text\"\n                icon=\"el-icon-edit\"\n                @click=\"handleEdit(scope.$index, scope.row)\">编辑\n            </el-button>\n            <el-button\n                type=\"text\"\n                icon=\"el-icon-delete\"\n                @click=\"handleDelete(scope.$index, scope.row)\">删除\n            </el-button>\n          </template>\n        </el-table-column>\n      </el-table>\n\n      <el-form :model=\"ruleForm\" :rules=\"rules\" ref=\"ruleForm\" label-width=\"80px\" class=\"demo-ruleForm\" size=\"medium\">\n        <el-dialog\n            title=\"添加\"\n            :append-to-body='true'\n            :visible.sync=\"dialogAdd\"\n            :before-close=\"handleClose\">\n          <el-form-item label=\"商品类别\">\n            <el-select v-model=\"ruleForm.category\" placeholder=\"请选择商品类别\" prop=\"category\">\n              <el-option label=\"电脑\" value=\"电脑\"></el-option>\n              <el-option label=\"平板\" value=\"平板\"></el-option>\n              <el-option label=\"手机\" value=\"手机\"></el-option>\n              <el-option label=\"显示器\" value=\"显示器\"></el-option>\n              <el-option label=\"电子配件\" value=\"电子配件\"></el-option>\n            </el-select>\n          </el-form-item>\n          <el-form-item label=\"商品编号\">\n            <el-input v-model=\"ruleForm.id\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"商品名称\">\n            <el-input v-model=\"ruleForm.name\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"商品产地\">\n            <el-select v-model=\"ruleForm.area\">\n              <el-option label=\"江苏\" value=\"江苏\"></el-option>\n              <el-option label=\"浙江\" value=\"浙江\"></el-option>\n              <el-option label=\"江西\" value=\"江西\"></el-option>\n              <el-option label=\"安徽\" value=\"安徽\"></el-option>\n              <el-option label=\"贵州\" value=\"贵州\"></el-option>\n              <el-option label=\"四川\" value=\"四川\"></el-option>\n              <el-option label=\"甘肃\" value=\"甘肃\"></el-option>\n              <el-option label=\"青海\" value=\"青海\"></el-option>\n              <el-option label=\"澳门\" value=\"澳门\"></el-option>\n              <el-option label=\"香港\" value=\"香港\"></el-option>\n              <el-option label=\"新疆\" value=\"新疆\"></el-option>\n              <el-option label=\"内蒙古\" value=\"内蒙古\"></el-option>\n              <el-option label=\"西藏\" value=\"西藏\"></el-option>\n              <el-option label=\"湖北\" value=\"湖北\"></el-option>\n              <el-option label=\"湖南\" value=\"湖南\"></el-option>\n              <el-option label=\"河北\" value=\"河北\"></el-option>\n              <el-option label=\"河南\" value=\"河南\"></el-option>\n              <el-option label=\"台湾\" value=\"台湾\"></el-option>\n              <el-option label=\"陕西\" value=\"陕西\"></el-option>\n              <el-option label=\"重庆\" value=\"重庆\"></el-option>\n              <el-option label=\"上海\" value=\"上海\"></el-option>\n              <el-option label=\"天津\" value=\"天津\"></el-option>\n              <el-option label=\"北京\" value=\"北京\"></el-option>\n              <el-option label=\"山西\" value=\"山西\"></el-option>\n              <el-option label=\"山东\" value=\"山东\"></el-option>\n              <el-option label=\"福建\" value=\"福建\"></el-option>\n              <el-option label=\"辽宁\" value=\"辽宁\"></el-option>\n              <el-option label=\"吉林\" value=\"吉林\"></el-option>\n              <el-option label=\"黑龙江\" value=\"黑龙江\"></el-option>\n              <el-option label=\"宁夏\" value=\"宁夏\"></el-option>\n              <el-option label=\"广西\" value=\"广西\"></el-option>\n              <el-option label=\"广东\" value=\"广东\"></el-option>\n            </el-select>\n          </el-form-item>\n          <el-form-item label=\"商品价格\">\n            <el-input v-model=\"ruleForm.price\">\n              <template slot=\"append\">元</template>\n            </el-input>\n          </el-form-item>\n          <el-form-item label=\"生产时间\">\n            <el-input v-model=\"ruleForm.dates\">\n              <template slot=\"append\">年/月/日</template>\n            </el-input>\n          </el-form-item>\n          <el-form-item label=\"生产商\">\n            <el-select v-model=\"ruleForm.producer\" placeholder=\"请选择生产商\" prop=\"producer\">\n              <el-option label=\"惠普\" value=\"惠普\"></el-option>\n              <el-option label=\"苹果\" value=\"苹果\"></el-option>\n              <el-option label=\"联想\" value=\"联想\"></el-option>\n              <el-option label=\"华为\" value=\"华为\"></el-option>\n              <el-option label=\"戴尔\" value=\"戴尔\"></el-option>\n              <el-option label=\"华硕\" value=\"华硕\"></el-option>\n              <el-option label=\"小米\" value=\"小米\"></el-option>\n              <el-option label=\"宏碁\" value=\"宏碁\"></el-option>\n              <el-option label=\"微星\" value=\"微星\"></el-option>\n            </el-select>\n          </el-form-item>\n\n          <span slot=\"footer\" class=\"dialog-footer\">\n            <el-button @click=\"emptyUserData()\" size=\"medium\">取 消</el-button>\n            <el-button @click=\"addUser('ruleForm')\" type=\"primary\" size=\"medium\">确 定</el-button>\n          </span>\n        </el-dialog>\n      </el-form>\n\n      <el-form :model=\"ruleForm\" :rules=\"rules\" ref=\"ruleForm\" label-width=\"70px\" class=\"demo-ruleForm\" size=\"medium\">\n        <el-dialog\n            title=\"编辑\"\n            :append-to-body='true'\n            :visible.sync=\"dialogUpdate\"\n            :before-close=\"handleClose\">\n          <el-form-item label=\"商品类别\">\n            <el-select v-model=\"ruleForm.category\" placeholder=\"请选择商品类别\" prop=\"category\">\n              <el-option label=\"电脑\" value=\"电脑\"></el-option>\n              <el-option label=\"平板\" value=\"平板\"></el-option>\n              <el-option label=\"手机\" value=\"手机\"></el-option>\n              <el-option label=\"显示器\" value=\"显示器\"></el-option>\n              <el-option label=\"电子配件\" value=\"电子配件\"></el-option>\n            </el-select>\n          </el-form-item>\n          <el-form-item label=\"商品编号\">\n            <el-input v-model=\"ruleForm.id\" :disabled=\"true\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"商品名称\">\n            <el-input v-model=\"ruleForm.name\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"商品产地\">\n            <el-select v-model=\"ruleForm.area\">\n              <el-option label=\"江苏\" value=\"江苏\"></el-option>\n              <el-option label=\"浙江\" value=\"浙江\"></el-option>\n              <el-option label=\"江西\" value=\"江西\"></el-option>\n              <el-option label=\"安徽\" value=\"安徽\"></el-option>\n              <el-option label=\"贵州\" value=\"贵州\"></el-option>\n              <el-option label=\"四川\" value=\"四川\"></el-option>\n              <el-option label=\"甘肃\" value=\"甘肃\"></el-option>\n              <el-option label=\"青海\" value=\"青海\"></el-option>\n              <el-option label=\"澳门\" value=\"澳门\"></el-option>\n              <el-option label=\"香港\" value=\"香港\"></el-option>\n              <el-option label=\"新疆\" value=\"新疆\"></el-option>\n              <el-option label=\"内蒙古\" value=\"内蒙古\"></el-option>\n              <el-option label=\"西藏\" value=\"西藏\"></el-option>\n              <el-option label=\"湖北\" value=\"湖北\"></el-option>\n              <el-option label=\"湖南\" value=\"湖南\"></el-option>\n              <el-option label=\"河北\" value=\"河北\"></el-option>\n              <el-option label=\"河南\" value=\"河南\"></el-option>\n              <el-option label=\"台湾\" value=\"台湾\"></el-option>\n              <el-option label=\"陕西\" value=\"陕西\"></el-option>\n              <el-option label=\"重庆\" value=\"重庆\"></el-option>\n              <el-option label=\"上海\" value=\"上海\"></el-option>\n              <el-option label=\"天津\" value=\"天津\"></el-option>\n              <el-option label=\"北京\" value=\"北京\"></el-option>\n              <el-option label=\"山西\" value=\"山西\"></el-option>\n              <el-option label=\"山东\" value=\"山东\"></el-option>\n              <el-option label=\"福建\" value=\"福建\"></el-option>\n              <el-option label=\"辽宁\" value=\"辽宁\"></el-option>\n              <el-option label=\"吉林\" value=\"吉林\"></el-option>\n              <el-option label=\"黑龙江\" value=\"黑龙江\"></el-option>\n              <el-option label=\"宁夏\" value=\"宁夏\"></el-option>\n              <el-option label=\"广西\" value=\"广西\"></el-option>\n              <el-option label=\"广东\" value=\"广东\"></el-option>\n            </el-select>\n          </el-form-item>\n          <el-form-item label=\"商品价格\">\n            <el-input v-model=\"ruleForm.price\">\n              <template slot=\"append\">元</template>\n            </el-input>\n          </el-form-item>\n          <el-form-item label=\"生产时间\">\n            <el-input v-model=\"ruleForm.dates\">\n              <template slot=\"append\">年/月/日</template>\n            </el-input>\n          </el-form-item>\n          <el-form-item label=\"生产商\">\n            <el-select v-model=\"ruleForm.producer\" placeholder=\"请选择生产商\" prop=\"producer\">\n              <el-option label=\"惠普\" value=\"惠普\"></el-option>\n              <el-option label=\"苹果\" value=\"苹果\"></el-option>\n              <el-option label=\"联想\" value=\"联想\"></el-option>\n              <el-option label=\"华为\" value=\"华为\"></el-option>\n              <el-option label=\"戴尔\" value=\"戴尔\"></el-option>\n              <el-option label=\"华硕\" value=\"华硕\"></el-option>\n              <el-option label=\"小米\" value=\"小米\"></el-option>\n              <el-option label=\"宏碁\" value=\"宏碁\"></el-option>\n              <el-option label=\"微星\" value=\"微星\"></el-option>\n            </el-select>\n          </el-form-item>\n\n          <span slot=\"footer\" class=\"dialog-footer\">\n            <el-button @click=\"emptyUserData()\" size=\"medium\">取 消</el-button>\n            <el-button @click=\"updateUser()\" type=\"primary\" size=\"medium\">确 定</el-button>\n          </span>\n        </el-dialog>\n      </el-form>\n      <br>\n\n      <el-pagination\n          background\n          :disabled=\"disablePage\"\n          :current-page.sync=\"currentPage\"\n          small\n          layout=\"prev, pager, next\"\n          :page-size=\"pageSize\"\n          :total=\"total\"\n          @current-change=\"handlePageChange\">\n      </el-pagination>\n    </el-card>\n  </div>\n</template>\n\n<script>\n\nexport default {\n  data() {\n    return {\n      ruleForm: {\n        id: '',\n        name: '',\n        category: '',\n        area: '',\n        price: '',\n        dates: '',\n        producer: ''\n      },\n      rules: {},\n      tableData: [],\n      search1: \"\",\n      search2: \"\",\n      dialogAdd: false,\n      dialogUpdate: false,\n      pageSize: 5,\n      currentPage: 1,\n      total: 0,\n      disablePage: false,\n      multipleSelection: []\n    };\n  },\n\n  created() {\n    let postData1 = {\n      page: this.currentPage,\n      name: this.search1,\n      category: this.search2\n    };\n    this.request({\n      method: 'post',\n      url: '/goods/selectGoodsPage',\n      data: postData1\n    }).then(response => {\n\n      this.tableData = response;\n    }).catch(error => {\n      console.log(error);\n    });\n\n    let postData = {\n      name: this.search1,\n      category: this.search2\n    };\n    this.request({\n      method: 'post',\n      url: '/goods/getRowCount',\n      data: postData\n    }).then(response => {\n      this.total = response;\n    }).catch(error => {\n      console.log(error);\n    });\n\n  },\n\n  methods: {\n    exportData() {\n      this.request({\n        method: \"post\",\n        url: \"/goods/export\",\n        params: {},\n        responseType: \"blob\"\n      }).then(res => {\n        let blob = new Blob([res], {type: \"application/vnd.ms-excel\"}); // 将服务端返回的文件流（二进制）excel文件转化为blob\n        let fileName = \"商品列表.xls\";\n        if (window.navigator && window.navigator.msSaveOrOpenBlob) {\n          // IE\n          window.navigator.msSaveOrOpenBlob(blob, fileName);\n        } else {\n          let objectUrl = (window.URL || window.webkitURL).createObjectURL(\n              blob\n          );\n          let downFile = document.createElement(\"a\");\n          downFile.style.display = \"none\";\n          downFile.href = objectUrl;\n          downFile.download = fileName; // 下载后文件名\n          document.body.appendChild(downFile);\n          downFile.click();\n          document.body.removeChild(downFile); // 下载完成移除元素\n          // window.location.href = objectUrl\n          window.URL.revokeObjectURL(objectUrl); // 只要映射存在，Blob就不能进行垃圾回收，因此一旦不再需要引用，就必须小心撤销URL，释放掉blob对象。\n        }\n      }).catch(err => {\n        console.log(err);\n      });\n    },\n    /**\n     * 分页\n     */\n    handlePageChange() {\n      //console.log(`当前页: ${this.currentPage}`);\n      let postData = {\n        page: this.currentPage,\n        name: this.search1,\n        category: this.search2\n      };\n      this.request({\n        method: 'post',\n        url: '/goods/selectGoodsPage',\n        data: postData\n      }).then(response => {\n        if (response === null) {\n          let postData1 = {\n            page: 1,\n            name: this.search1,\n            category: this.search2\n          };\n          this.request({\n            method: 'post',\n            url: '/goods/selectGoodsPage',\n            data: postData1\n          }).then(response => {\n            this.tableData = response;\n          })\n        } else {\n          this.tableData = response;\n        }\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    /**\n     * 添加\n     */\n    addUser() {\n      if (this.ruleForm.category == null || this.ruleForm.id == null || this.ruleForm.name == null || this.ruleForm.area == null || this.ruleForm.price == null || this.ruleForm.dates == null || this.ruleForm.producer == null) {\n        this.$alert('商品信息不完整请检查', '温馨提示', {\n          confirmButtonText: '确定'\n        });\n        return;\n      }\n      this.$refs.ruleForm.validate(async valid => {\n        if (!valid) return;\n        console.log(this.ruleForm);\n        const {msg: res} = await this.request.post(\"/goods/createGoods\", this.ruleForm); // 访问后台，用idea中this.requestMapping映射login到这里\n        if (res === \"success\") {\n          this.$message.success(\"添加成功！\"); // 信息提示\n          location.reload();\n        } else {\n          this.$message.error(\"添加失败！\"); // 错误提示\n        }\n      });\n    },\n\n    /**\n     * 统计用户个数\n     */\n    getRowCount() {\n      let postData = {\n        name: this.search1,\n        category: this.search2\n      };\n      this.request({\n        method: 'post',\n        url: '/goods/getRowCount',\n        data: postData\n      }).then(response => {\n        this.total = response;\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    handleSearch() {\n      let postData = {\n        name: this.search1,\n        category: this.search2\n      };\n      this.request({\n        method: 'post',\n        url: '/goods/getRowCount',\n        data: postData\n      }).then(response => {\n        this.total = response;\n        let countPage = Math.ceil(this.total / 5);\n        if (countPage <= this.currentPage) {\n          this.currentPage = countPage;\n        }\n        this.handlePageChange();\n      }).catch(error => {\n        this.$message.error(\"未查询到符合条件商品！\");\n      });\n    },\n\n    handleEdit(index, row) {\n      this.dialogUpdate = true;\n      row.dates = Number(row.dates);\n      this.ruleForm = Object.assign({}, row, index);\n    },\n\n    handleClose(done) {\n      this.$confirm('确认关闭？')\n          .then(_ => {\n            done();\n            this.emptyUserData();\n          })\n          .catch(_ => {\n          });\n    },\n\n    /**\n     * 清空绑定数据\n     */\n    emptyUserData() {\n      this.dialogAdd = false;\n      this.dialogUpdate = false;\n      this.ruleForm = {\n        id: null,\n        name: null,\n        category: null,\n        area: null,\n        price: null,\n        dates: null,\n        producer: null\n      };\n    },\n\n    /**\n     * 根据 id 删除\n     * @param index\n     * @param row\n     */\n    handleDelete(index, row) {\n      //console.log(index, row);\n      this.$confirm('删除操作, 是否继续?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        let postData = {\n          id: row.id,\n        };\n        this.request({\n          method: 'post',\n          url: '/goods/deleteGoodsById',\n          data: postData\n        }).then(response => {\n          this.getRowCount();\n          if (this.total % 5 === 1 && this.currentPage >= 1) {\n            if (this.total / 5 < this.currentPage) {\n              this.currentPage = this.currentPage - 1;\n            }\n          }\n          this.handlePageChange();\n\n          this.$message({\n            type: 'success',\n            message: '删除成功!'\n          });\n          //console.log(response);\n        }).catch(error => {\n          console.log(error);\n        });\n\n      }).catch(() => {\n        this.$message({\n          type: 'info',\n          message: '已取消删除'\n        });\n      });\n    },\n\n    handleSelectionDelete(val) {\n      this.multipleSelection = val;\n    },\n\n    /**\n     * 根据id 批量删除\n     */\n    handleDeleteList() {\n      let userIds = \"\";\n      this.multipleSelection.forEach(item => {\n        userIds += item.id + ',';\n      })\n      console.log(userIds);\n\n      this.$confirm('删除操作, 是否继续?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        let postData = {\n          userIdList: userIds\n        };\n        console.log(postData);\n        this.request({\n          method: 'post',\n          url: '/goods/deleteGoodsByIdList',\n          data: postData\n        }).then(response => {\n          this.getRowCount();\n          if (this.total % 5 === 1 && this.currentPage >= 1) {\n            if (this.total / 5 < this.currentPage) {\n              this.currentPage = this.currentPage - 1;\n            }\n          }\n          this.handlePageChange();\n\n          this.$message({\n            type: 'success',\n            message: '删除成功!'\n          });\n        }).catch(error => {\n          console.log(error);\n        });\n\n      }).catch(() => {\n        this.$message({\n          type: 'info',\n          message: '已取消删除'\n        });\n      });\n    },\n\n\n    updateUser() {\n      if (this.ruleForm.category == null || this.ruleForm.id == null || this.ruleForm.name == null || this.ruleForm.area == null || this.ruleForm.price == null || this.ruleForm.dates == null || this.ruleForm.producer == null) {\n        this.$alert('商品信息不完整请检查', '温馨提示', {\n          confirmButtonText: '确定'\n        });\n        return;\n      }\n      let postData = {\n        id: this.ruleForm.id,\n        category: this.ruleForm.category,\n        name: this.ruleForm.name,\n        area: this.ruleForm.area,\n        price: this.ruleForm.price,\n        dates: this.ruleForm.dates,\n        producer: this.ruleForm.producer,\n      };\n      this.request({\n        method: 'post',\n        url: '/goods/updateGoodsById',\n        data: postData\n      }).then(response => {\n        this.handlePageChange();\n        this.getRowCount();\n        this.$message({\n          type: 'success',\n          message: '已编辑!'\n        });\n        this.emptyUserData();\n        //console.log(response);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    loginout() {\n      window.sessionStorage.clear();\n      this.$router.push(\"/login\");\n    },\n  },\n\n}\n</script>\n<style scoped>\n</style>\n"]}]}